{
  "name": "ember-browser-services",
  "version": "0.0.0",
  "description": "Browser APIs as services for easier testing",
  "keywords": [
    "ember-addon",
    "browser",
    "ember",
    "ember.js",
    "services",
    "window",
    "testing"
  ],
  "repository": "https://github.com/CrowdStrike/ember-browser-services",
  "license": "MIT",
  "author": "CrowdStrike UX Team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:docs": "remark .",
    "lint:docs-js": "eslint . --cache --ext md",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "tsc --build",
    "postpublish": "tsc --build --clean"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "typesVersions": {
    "*": {
      "*": [
        "declarations/*",
        "declarations/*/index"
      ]
    }
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@types/ember": "3.16.2",
    "@types/ember-qunit": "3.4.13",
    "@types/ember-resolver": "5.0.10",
    "@types/ember__test-helpers": "1.7.3",
    "@types/qunit": "2.9.6",
    "@types/rsvp": "4.0.3",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-decorator-position": "^2.2.11",
    "eslint-plugin-ember": "^9.6.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-qunit": "^5.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "qunit-dom": "^1.6.0",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "typescript": "^4.0.5"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "12.20.2",
    "yarn": "1.22.10"
  }
}
